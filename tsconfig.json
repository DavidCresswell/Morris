{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "es2016",
        "jsx": "preserve",
        "resolveJsonModule": true,
        "outDir": "out",
        "sourceMap": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
